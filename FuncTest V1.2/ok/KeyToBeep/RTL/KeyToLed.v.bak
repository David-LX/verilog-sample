module KeyToLed( 
              //input 
              key,
              //output 
              led
              );

//input ports
input  [3:0]             key;

//output ports
output [3:0]             led;

//reg define 

//wire define 
wire    [3:0]            key;
wire   [3:0]             led;

//parameter define 

/*******************************************************************************************************
**                              Main Program    
**  
********************************************************************************************************/

assign led = key;                //key value display in led

endmodule
//end of RTL code                       