module KeyToLED( 
//input 
input         [3:0]       key  ,
//output 
output wire   [7:0]       LED  

              );

//input ports

//output ports

//reg define 

//parameter define 

/*******************************************************************************************************
**                              Main Program    
**  
********************************************************************************************************/

assign LED = ~{ key, key } ;                //key value display in beep

endmodule
//end of RTL code                       
