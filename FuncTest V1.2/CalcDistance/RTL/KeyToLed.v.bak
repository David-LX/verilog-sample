module KeyToLED( 
              //input 
              key,
              //output 
              LED
              );

//input ports
input         [7:0]              key  ;

//output ports
output wire   [7:0]              LED ;

//reg define 

//parameter define 

/*******************************************************************************************************
**                              Main Program    
**  
********************************************************************************************************/

assign LED = ~{ key, key } ;                //key value display in beep

endmodule
//end of RTL code                       